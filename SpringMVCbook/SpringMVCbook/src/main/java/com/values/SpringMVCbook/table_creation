CREATE TABLE books (
  id SERIAL PRIMARY KEY,
  name VARCHAR(255) NOT NULL,
  description VARCHAR(255),
  image TEXT,
  quantita INTEGER NOT NULL,
  price DOUBLE PRECISION NOT NULL
);

